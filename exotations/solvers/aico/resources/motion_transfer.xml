<?xml version="1.0" ?>
<ExampleConfiguration>
  <AICOsolver name="MyAICOsolver">
    <sweepMode>Symmetric</sweepMode>
    <max_iterations>100</max_iterations>
    <tolerance>1e-3</tolerance>
    <damping>0.01</damping>
    <UseBackwardMessage>0</UseBackwardMessage>
    <dynamic>0</dynamic>
  </AICOsolver>

  <AICOProblem name="ReachingProblem">
    <KScene name="EffScene">
      <Kinematica>
        <Urdf>/home/yiming/ros/slmc/core_utilities/src/aico/resources/atlas.urdf</Urdf> <!--The URDF containing the tree structure-->
        <Root segment="pelvis"> <!-- Optional: The root segment to use: xyz -->
          <vector>0.0 0.0 0.0</vector>  <!-- x y z-->
          <quaternion>1.0 0.0 0.0 0.0</quaternion> <!-- w x y z-->
        </Root>
        <Update zero_unnamed="true">  <!--compulsary: the list of joints which will be updated: IF flag not present, defaults to true-->
          <joint name="back_bkz"/>
          <joint name="back_bky"/>
          <joint name="back_bkx"/> 
          <joint name="l_arm_shy"/> 
          <joint name="l_arm_shx"/> 
          <joint name="l_arm_ely"/> 
          <joint name="l_arm_elx"/> 
          <joint name="l_arm_wry"/> 
          <joint name="l_arm_wrx"/>
          <joint name="r_arm_shy"/> 
          <joint name="r_arm_shx"/> 
          <joint name="r_arm_ely"/> 
          <joint name="r_arm_elx"/> 
          <joint name="r_arm_wry"/> 
          <joint name="r_arm_wrx"/> 
        </Update>
        <EndEffector ignore_unused="true">  <!--If not present, defaults to false-->
          <limb segment="l_hand"> <!--Compulsary-->
            <vector>0.0 0.0 0.0</vector>  <!-- Optional: Defaults to zero-vector -->
            <quaternion>1.0 0.0 0.0 0.0</quaternion> <!-- Optional: Defaults to identity quaternion -->
          </limb>

        </EndEffector>
      </Kinematica>
      
    </KScene>
    

    <KScene name="IMeshScene">
      <Kinematica>
        <Urdf>/home/yiming/ros/slmc/core_utilities/src/aico/resources/atlas.urdf</Urdf> <!--The URDF containing the tree structure-->
        <Root segment="pelvis"> <!-- Optional: The root segment to use: xyz -->
          <vector>0 0 0</vector>  <!-- x y z-->
          <quaternion>1.0 0 0 0</quaternion> <!-- w x y z-->
        </Root>
        <Update zero_unnamed="true">  <!--compulsary: the list of joints which will be updated: IF flag not present, defaults to true-->
          <joint name="back_bkz"/>
          <joint name="back_bky"/>
          <joint name="back_bkx"/> 
          <joint name="l_arm_shy"/> 
          <joint name="l_arm_shx"/> 
          <joint name="l_arm_ely"/> 
          <joint name="l_arm_elx"/> 
          <joint name="l_arm_wry"/> 
          <joint name="l_arm_wrx"/>
          <joint name="r_arm_shy"/> 
          <joint name="r_arm_shx"/> 
          <joint name="r_arm_ely"/> 
          <joint name="r_arm_elx"/> 
          <joint name="r_arm_wry"/> 
          <joint name="r_arm_wrx"/> 
        </Update>

        <EndEffector ignore_unused="true"> 
          <limb segment="right_palm"><vector>0 0 0</vector></limb>          
          <limb segment="r_larm"></limb>
          <limb segment="r_clav"><vector>0 0 0</vector></limb>
          <limb segment="left_palm"><vector>0 0 0</vector></limb>          
          <limb segment="l_larm"></limb>
          <limb segment="l_clav"><vector>0 0 0</vector></limb>
          <limb segment="pelvis"><vector>0 0 0</vector></limb>
        </EndEffector>

      </Kinematica>
      
    </KScene> 


    <Map type="EffPosition" name="LeftHandPositionMap">
      <kscene name="EffScene"/>
      <!-- Map Parameters -->
    </Map>
    
    <Task name="LeftHandPosition" type="TaskSqrError">
      <map name="LeftHandPositionMap"/>
      <Rho>10.0</Rho>
      <Weights dim="1">1.0</Weights>
      <Goal>0.7 0.0 0.0</Goal>
      <!-- Task Parameters -->
    </Task>

    <Map type="IMesh" name="MotionTransferMap">
      <kscene name="IMeshScene"/>
      <SafeRange>0.3</SafeRange>
      <MaxGraph>7</MaxGraph>
    </Map>
    
    <Task name="MotionTransfer" type="TaskSqrError">
      <map name="MotionTransferMap"/>
      <Rho>1.0</Rho>
      <Weights dim="1">1.0</Weights>
      <Goal>0.252613  -0.329363   0.111133 -0.230774 -0.0772617   0.186181   0.357827   0.211831  -0.215156   0.111725   0.228898 -0.0578643 -0.0971091   0.195655  0.0996829  -0.405011  0.0390478         -0.379114 0 0 0
</Goal>
    </Task>
   
    <!-- Problem parameters (structuring etc...) -->
    <T>4</T>
    <duration>5.0</duration>
    <Qrate>1e-10</Qrate>
    <Hrate>1.0</Hrate>
    <Wrate>.5</Wrate>
    <W> 9 8 7 6 5 4 3 2 1 6 5 4 3 2 1</W>
  </AICOProblem>



</ExampleConfiguration>
