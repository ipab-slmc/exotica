<?xml version="1.0"?>
<package format="2">
  <name>ompl_solver</name>
  <version>2.0.0</version>
  <description>The Open Motion Planning Libary (OMPL) solver</description>
  <maintainer email="yiming.yang@ed.ac.uk">Yiming Yang</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>eigen_catkin</build_depend>

  <depend>exotica</depend>
  <depend>ompl</depend>
  <depend>exotica_python</depend>

  <export>
    <exotica plugin="${prefix}/exotica_plugins.xml" />
  </export>
</package>
