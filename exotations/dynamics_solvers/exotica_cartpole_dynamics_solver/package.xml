<?xml version="1.0"?>
<package format="2">
  <name>exotica_cartpole_dynamics_solver</name>
  <version>6.0.2</version>
  <description>Cartpole dynamics solver plug-in for Exotica</description>

  <maintainer email="traiko.dinev@ed.ac.uk">Traiko Dinev</maintainer>
  <maintainer email="wolfgang@robots.ox.ac.uk">Wolfgang Merkt</maintainer>
  <author>Traiko Dinev</author>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>exotica_core</build_depend>
  <build_depend>roscpp</build_depend>
  <build_export_depend>exotica_core</build_export_depend>
  <build_export_depend>roscpp</build_export_depend>
  <exec_depend>exotica_core</exec_depend>
  <exec_depend>roscpp</exec_depend>
  <!-- not currently used (test deactivated, installation fails on kinetic <test_depend>autograd-pip</test_depend>-->
  <test_depend>exotica_python</test_depend>

  <export>
    <exotica_core plugin="${prefix}/exotica_plugins.xml" />
  </export>
</package>
